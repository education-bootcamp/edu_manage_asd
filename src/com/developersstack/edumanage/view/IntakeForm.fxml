<AnchorPane fx:id="context" prefHeight="482.0" prefWidth="737.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.developersstack.edumanage.controller.IntakeFormController">
   <children>
      <Label layoutX="34.0" layoutY="34.0" text="Intake Management" textFill="#160090">
